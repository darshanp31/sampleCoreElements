{
	"_version": "1.12.0",
	"sap.app": {
		"id": "sampleCoreElements.sampleCoreElements",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		},
		"dataSources": {
			"optionForUsersDataSrc": {
				"uri": "model/data.json",
				"type": "JSON"
			},
			"globalModelSrc": {
				"uri": "model/dataOverView.json",
				"type": "JSON"
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "sampleCoreElements.sampleCoreElements.view.app",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.65.6",
			"libs": {
				"sap.m": {},
				"sap.suite.ui.microchart": {
					"minVersion": ""
				},
				"sap.ui.codeeditor": {
					"minVersion": ""
				},
				"sap.ui.commons": {
					"minVersion": ""
				},
				"sap.ui.comp": {
					"minVersion": ""
				},
				"sap.ui.core": {},
				"sap.ui.documentation": {
					"minVersion": ""
				},
				"sap.ui.dt": {
					"minVersion": ""
				},
				"sap.ui.export": {
					"minVersion": ""
				},
				"sap.ui.generic.app": {
					"minVersion": ""
				},
				"sap.ui.generic.template": {
					"minVersion": ""
				},
				"sap.ui.integration": {
					"minVersion": ""
				},
				"sap.ui.layout": {},
				"sap.ui.table": {
					"minVersion": ""
				},
				"sap.ui.ux3": {
					"minVersion": ""
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sampleCoreElements.sampleCoreElements.i18n.i18n"
				}
			},
			"optionForUsers": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "optionForUsersDataSrc"
			},
			"globalModel": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "globalModelSrc"
			}
		},
		"resources": {
			"css": [
				{
					"uri": "css/style.css"
				}
			],
			"js": [{
				
				"uri": "util/formatter.js"
				
			}]
			
			
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sampleCoreElements.sampleCoreElements.view",
				"controlAggregation": "pages",
				"controlId": "App"
			},
			"routes": [
				{
					"pattern": "",
					"name": "home",
					"target": "home"
				},
				{
					"pattern": "materialoverview",
					"name": "materialoverview",
					"target": "materialoverview"
				}
			],
			"targets": {
				"home": {
					"viewName": "homePage",
					"viewPath": "sampleCoreElements.sampleCoreElements.view",
					"viewLevel": 1
				},
				"materialoverview": {
					"viewPath": "sampleCoreElements.sampleCoreElements.view",
					"viewName": "materialOverView",
					"viewLevel": 2
				}
			}
		}
	}
}